\title{Generalized PROs}
\taxon{theory}
\import{macros}

\p{The \define{theory of generalized PROs} is the modal double theory #{(\dbl{T}, T)} generated by

\ul{
  \li{an object #{x}}
  \li{a proarrow #{m: Tx \proto Tx}}
  \li{a pair of cells, the \define{product} and the \define{unit}
  \quiver{
  % https://q.uiver.app/#
  \begin{tikzcd}
	{T^2 x} & {T^2 x} && x & x \\
	Tx & Tx && Tx & Tx
	\arrow[""{name=0, anchor=center, inner sep=0}, "Tm"{inner sep=.8ex}, "\shortmid"{marking}, from=1-1, to=1-2]
	\arrow["{\mu_x}"', from=1-1, to=2-1]
	\arrow["{\mu_x}", from=1-2, to=2-2]
	\arrow[""{name=1, anchor=center, inner sep=0}, "{\mathrm{id}_x}"{inner sep=.8ex}, "\shortmid"{marking}, from=1-4, to=1-5]
	\arrow["{\eta_x}"', from=1-4, to=2-4]
	\arrow["{\eta_x}", from=1-5, to=2-5]
	\arrow[""{name=2, anchor=center, inner sep=0}, "m"'{inner sep=.8ex}, "\shortmid"{marking}, from=2-1, to=2-2]
	\arrow[""{name=3, anchor=center, inner sep=0}, "m"'{inner sep=.8ex}, "\shortmid"{marking}, from=2-4, to=2-5]
	\arrow["\otimes"{description}, draw=none, from=0, to=2]
	\arrow["\eta"{description}, draw=none, from=1, to=3]
  \end{tikzcd}
  }
  } 
}

subject to the equations

\ul{
  \li{\em{Composition}: #{m \odot m = m}}
  \li{\em{Functorality of product}: #{\otimes \odot \otimes = \otimes}, i.e.,
  \quiver{
  % https://q.uiver.app/#
  \begin{tikzcd}
	{T^2 x} & {T^2 x} & {T^2 x} && {T^2 x} & {T^2 x} \\
	Tx & Tx & Tx && Tx & Tx
	\arrow[""{name=0, anchor=center, inner sep=0}, "Tm"{inner sep=.8ex}, "\shortmid"{marking}, from=1-1, to=1-2]
	\arrow["{\mu_x}"', from=1-1, to=2-1]
	\arrow[""{name=1, anchor=center, inner sep=0}, "Tm"{inner sep=.8ex}, "\shortmid"{marking}, from=1-2, to=1-3]
	\arrow["{\mu_x}"{description}, from=1-2, to=2-2]
	\arrow[""{name=2, anchor=center, inner sep=0}, "{\mu_x}", from=1-3, to=2-3]
	\arrow[""{name=3, anchor=center, inner sep=0}, "Tm"{inner sep=.8ex}, "\shortmid"{marking}, from=1-5, to=1-6]
	\arrow[""{name=4, anchor=center, inner sep=0}, "{\mu_x}"', from=1-5, to=2-5]
	\arrow["{\mu_x}", from=1-6, to=2-6]
	\arrow[""{name=5, anchor=center, inner sep=0}, "m"'{inner sep=.8ex}, "\shortmid"{marking}, from=2-1, to=2-2]
	\arrow[""{name=6, anchor=center, inner sep=0}, "m"'{inner sep=.8ex}, "\shortmid"{marking}, from=2-2, to=2-3]
	\arrow[""{name=7, anchor=center, inner sep=0}, "m"'{inner sep=.8ex}, "\shortmid"{marking}, from=2-5, to=2-6]
	\arrow["\otimes"{description}, draw=none, from=0, to=5]
	\arrow["\otimes"{description}, draw=none, from=1, to=6]
	\arrow["{=}"{description}, draw=none, from=2, to=4]
	\arrow["\otimes"{description}, draw=none, from=3, to=7]
  \end{tikzcd}
  }
  }
  \li{\em{Functorality of unit}: #{\eta \odot \eta = \eta}}
  \li{\em{Associativity of product}:
  \quiver{
  % https://q.uiver.app/#
  \begin{tikzcd}
	{T^3 x} & {T^3 x} && {T^3 x} & {T^3 x} \\
	{T^2 x} & {T^2 x} && {T^2 x} & {T^2 x} \\
	Tx & Tx && Tx & Tx
	\arrow[""{name=0, anchor=center, inner sep=0}, "{T^2 m}"{inner sep=.8ex}, "\shortmid"{marking}, from=1-1, to=1-2]
	\arrow["{T\mu_x}"', from=1-1, to=2-1]
	\arrow["{T\mu_x}", from=1-2, to=2-2]
	\arrow[""{name=1, anchor=center, inner sep=0}, "{T^2 m}"{inner sep=.8ex}, "\shortmid"{marking}, from=1-4, to=1-5]
	\arrow["{\mu_{Tx}}"', from=1-4, to=2-4]
	\arrow["{\mu_{Tx}}", from=1-5, to=2-5]
	\arrow[""{name=2, anchor=center, inner sep=0}, "Tm"'{inner sep=.8ex}, "\shortmid"{marking}, from=2-1, to=2-2]
	\arrow["{\mu_x}"', from=2-1, to=3-1]
	\arrow["{=}"{description}, draw=none, from=2-2, to=2-4]
	\arrow["{\mu_x}", from=2-2, to=3-2]
	\arrow[""{name=3, anchor=center, inner sep=0}, "Tm"'{inner sep=.8ex}, "\shortmid"{marking}, from=2-4, to=2-5]
	\arrow["{\mu_x}"', from=2-4, to=3-4]
	\arrow["{\mu_x}", from=2-5, to=3-5]
	\arrow[""{name=4, anchor=center, inner sep=0}, "m"'{inner sep=.8ex}, "\shortmid"{marking}, no head, from=3-1, to=3-2]
	\arrow[""{name=5, anchor=center, inner sep=0}, "m"'{inner sep=.8ex}, "\shortmid"{marking}, from=3-4, to=3-5]
	\arrow["{T\otimes}"{description}, draw=none, from=0, to=2]
	\arrow["{\mu_m}"{description}, draw=none, from=1, to=3]
	\arrow["\otimes"{description, pos=0.6}, draw=none, from=2, to=4]
	\arrow["\otimes"{description, pos=0.6}, draw=none, from=3, to=5]
  \end{tikzcd}
  }
  }
  \li{\em{Unitality of product}:
  \quiver{
  % https://q.uiver.app/#
  \begin{tikzcd}
	Tx & Tx && Tx & Tx \\
	{T^2 x} & {T^2 x} \\
	Tx & Tx && Tx & Tx
	\arrow[""{name=0, anchor=center, inner sep=0}, "m"{inner sep=.8ex}, "\shortmid"{marking}, from=1-1, to=1-2]
	\arrow["{\eta_{Tx}}"', from=1-1, to=2-1]
	\arrow["{\eta_{Tx}}", from=1-2, to=2-2]
	\arrow[""{name=1, anchor=center, inner sep=0}, "m"{inner sep=.8ex}, "\shortmid"{marking}, from=1-4, to=1-5]
	\arrow[""{name=2, anchor=center, inner sep=0}, equals, from=1-4, to=3-4]
	\arrow[equals, from=1-5, to=3-5]
	\arrow[""{name=3, anchor=center, inner sep=0}, "Tm"'{inner sep=.8ex}, "\shortmid"{marking}, from=2-1, to=2-2]
	\arrow["{\mu_x}"', from=2-1, to=3-1]
	\arrow["{\mu_x}", from=2-2, to=3-2]
	\arrow[""{name=4, anchor=center, inner sep=0}, "m"'{inner sep=.8ex}, "\shortmid"{marking}, from=3-1, to=3-2]
	\arrow[""{name=5, anchor=center, inner sep=0}, "m"'{inner sep=.8ex}, "\shortmid"{marking}, from=3-4, to=3-5]
	\arrow["{\eta_m}"{description}, draw=none, from=0, to=3]
	\arrow["{1_m}"{description}, draw=none, from=1, to=5]
	\arrow["\otimes"{description, pos=0.6}, draw=none, from=3, to=4]
	\arrow["{=}"{description}, draw=none, from=2-2, to=2]
  \end{tikzcd}
  }
  }
  \li{\em{Normalization}: The cell #{\eta} is a restriction, i.e., the unique
  globular cell #{\id_x \To m(\eta_x, \eta_x)} factoring #{\eta} through any
  chosen restriction is an isomorphism.}
}
}
