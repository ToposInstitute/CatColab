\title{Monoidal promonads}
\taxon{theory}
\import{macros}

\p{

The \define{theory of monoidal promonads} is the modal theory #{(\dbl{T},T)}
that combines the [theory of generalized monoidal categories](thy-0005) and the
[theory of promonads](thy-0004) with a cell

\quiver{
% https://q.uiver.app/#
\begin{tikzcd}
	Tx & Tx \\
	x & x
	\arrow[""{name=0, anchor=center, inner sep=0}, "Tp"{inner sep=.8ex}, "\shortmid"{marking}, from=1-1, to=1-2]
	\arrow["\otimes"', from=1-1, to=2-1]
	\arrow["\otimes", from=1-2, to=2-2]
	\arrow[""{name=1, anchor=center, inner sep=0}, "p"'{inner sep=.8ex}, "\shortmid"{marking}, from=2-1, to=2-2]
	\arrow["\otimes"{description}, draw=none, from=0, to=1]
\end{tikzcd}
}

subject to the axioms:

\ul{
  \li{\em{Compatibility of products}:
  \centerfigs{
  \quiverinline{
  % https://q.uiver.app/#
  \begin{tikzcd}
	Tx & Tx \\
	Tx & Tx \\
	x & x
	\arrow[""{name=0, anchor=center, inner sep=0}, "\shortmid"{marking}, equals, from=1-1, to=1-2]
	\arrow[equals, from=1-1, to=2-1]
	\arrow[equals, from=1-2, to=2-2]
	\arrow[""{name=1, anchor=center, inner sep=0}, "Tp"{inner sep=.8ex}, "\shortmid"{marking}, from=2-1, to=2-2]
	\arrow["\otimes"', from=2-1, to=3-1]
	\arrow["\otimes", from=2-2, to=3-2]
	\arrow[""{name=2, anchor=center, inner sep=0}, "p"'{inner sep=.8ex}, "\shortmid"{marking}, from=3-1, to=3-2]
	\arrow["{T\phi}"{description, pos=0.4}, draw=none, from=0, to=1]
	\arrow["\otimes"{description}, draw=none, from=1, to=2]
  \end{tikzcd}
  }
  #{\qquad=\qquad}
  \quiverinline{
  % https://q.uiver.app/#
  \begin{tikzcd}
	Tx & Tx \\
	x & x \\
	x & x
	\arrow[""{name=0, anchor=center, inner sep=0}, "\shortmid"{marking}, equals, from=1-1, to=1-2]
	\arrow["\otimes"', from=1-1, to=2-1]
	\arrow["\otimes", from=1-2, to=2-2]
	\arrow[""{name=1, anchor=center, inner sep=0}, "\shortmid"{marking}, equals, from=2-1, to=2-2]
	\arrow[equals, from=2-1, to=3-1]
	\arrow[equals, from=2-2, to=3-2]
	\arrow[""{name=2, anchor=center, inner sep=0}, "p"'{inner sep=.8ex}, "\shortmid"{marking}, from=3-1, to=3-2]
	\arrow["{\mathrm{id}_\otimes}"{description}, draw=none, from=0, to=1]
	\arrow["\phi"{description}, draw=none, from=1, to=2]
  \end{tikzcd}
  }
  }
  }
  \li{\em{Associativity}:
  \centerfigs{
  \quiverinline{
  % https://q.uiver.app/#
  \begin{tikzcd}
	{T^2x} & {T^2 x} & {T^2 x} \\
	Tx & Tx & Tx \\
	x & x & x
	\arrow[""{name=0, anchor=center, inner sep=0}, "{T^2 p}"{inner sep=.8ex}, "\shortmid"{marking}, from=1-1, to=1-2]
	\arrow["{T\otimes}"', from=1-1, to=2-1]
	\arrow[""{name=1, anchor=center, inner sep=0}, "\shortmid"{marking}, equals, from=1-2, to=1-3]
	\arrow["{T\otimes}", from=1-2, to=2-2]
	\arrow["{\mu_x}", from=1-3, to=2-3]
	\arrow[""{name=2, anchor=center, inner sep=0}, "Tp"{inner sep=.8ex}, "\shortmid"{marking}, from=2-1, to=2-2]
	\arrow["\otimes"', from=2-1, to=3-1]
	\arrow["\otimes", from=2-2, to=3-2]
	\arrow["\otimes", from=2-3, to=3-3]
	\arrow[""{name=3, anchor=center, inner sep=0}, "p"'{inner sep=.8ex}, "\shortmid"{marking}, from=3-1, to=3-2]
	\arrow[""{name=4, anchor=center, inner sep=0}, "\shortmid"{marking}, equals, from=3-2, to=3-3]
	\arrow["{T\otimes}"{description, pos=0.4}, draw=none, from=0, to=2]
	\arrow["\gamma"{description}, draw=none, from=1, to=4]
	\arrow["\otimes"{description}, draw=none, from=2, to=3]
  \end{tikzcd}
  }
  #{\qquad=\qquad}
  \quiverinline{
  % https://q.uiver.app/#
  \begin{tikzcd}
	{T^2 x} & {T^2 x} & {T^2 x} \\
	Tx & Tx & Tx \\
	x & x & x
	\arrow[""{name=0, anchor=center, inner sep=0}, "\shortmid"{marking}, equals, from=1-1, to=1-2]
	\arrow["{T\otimes}"', from=1-1, to=2-1]
	\arrow[""{name=1, anchor=center, inner sep=0}, "{T^2 p}"{inner sep=.8ex}, "\shortmid"{marking}, from=1-2, to=1-3]
	\arrow["{\mu_x}"', from=1-2, to=2-2]
	\arrow["{\mu_x}", from=1-3, to=2-3]
	\arrow["\otimes"', from=2-1, to=3-1]
	\arrow[""{name=2, anchor=center, inner sep=0}, "Tp"{inner sep=.8ex}, "\shortmid"{marking}, from=2-2, to=2-3]
	\arrow["\otimes"', from=2-2, to=3-2]
	\arrow["\otimes", from=2-3, to=3-3]
	\arrow[""{name=3, anchor=center, inner sep=0}, "\shortmid"{marking}, equals, from=3-1, to=3-2]
	\arrow[""{name=4, anchor=center, inner sep=0}, "p"'{inner sep=.8ex}, "\shortmid"{marking}, from=3-2, to=3-3]
	\arrow["\gamma"{description}, draw=none, from=0, to=3]
	\arrow["{\mu_p}"{description, pos=0.4}, draw=none, from=1, to=2]
	\arrow["\otimes"{description}, draw=none, from=2, to=4]
  \end{tikzcd}
  }
  }
  }
  \li{\em{Unitality}:
  \centerfigs{
  \quiverinline{
  % https://q.uiver.app/#
  \begin{tikzcd}
	x & x & x \\
	&& Tx \\
	x & x & x
	\arrow[""{name=0, anchor=center, inner sep=0}, "p"{inner sep=.8ex}, "\shortmid"{marking}, from=1-1, to=1-2]
	\arrow[equals, from=1-1, to=3-1]
	\arrow[""{name=1, anchor=center, inner sep=0}, "\shortmid"{marking}, equals, from=1-2, to=1-3]
	\arrow[equals, from=1-2, to=3-2]
	\arrow["{\eta_x}", from=1-3, to=2-3]
	\arrow["\otimes", from=2-3, to=3-3]
	\arrow[""{name=2, anchor=center, inner sep=0}, "p"'{inner sep=.8ex}, "\shortmid"{marking}, from=3-1, to=3-2]
	\arrow[""{name=3, anchor=center, inner sep=0}, "\shortmid"{marking}, equals, from=3-2, to=3-3]
	\arrow["{1_p}"{description}, draw=none, from=0, to=2]
	\arrow["\iota"{description}, draw=none, from=1, to=3]
  \end{tikzcd}
  }
  #{\qquad=\qquad}
  \quiverinline{
  % https://q.uiver.app/#
  \begin{tikzcd}
	x & x & x \\
	& Tx & Tx \\
	x & x & x
	\arrow[""{name=0, anchor=center, inner sep=0}, "\shortmid"{marking}, equals, from=1-1, to=1-2]
	\arrow[equals, from=1-1, to=3-1]
	\arrow[""{name=1, anchor=center, inner sep=0}, "p"{inner sep=.8ex}, "\shortmid"{marking}, from=1-2, to=1-3]
	\arrow["{\eta_x}"', from=1-2, to=2-2]
	\arrow["{\eta_x}", from=1-3, to=2-3]
	\arrow[""{name=2, anchor=center, inner sep=0}, "Tp"{inner sep=.8ex}, "\shortmid"{marking}, from=2-2, to=2-3]
	\arrow["\otimes"', from=2-2, to=3-2]
	\arrow["\otimes", from=2-3, to=3-3]
	\arrow[""{name=3, anchor=center, inner sep=0}, "\shortmid"{marking}, equals, from=3-1, to=3-2]
	\arrow[""{name=4, anchor=center, inner sep=0}, "p"'{inner sep=.8ex}, "\shortmid"{marking}, from=3-2, to=3-3]
	\arrow["\iota"{description}, draw=none, from=0, to=3]
	\arrow["{\eta_p}"{description, pos=0.4}, draw=none, from=1, to=2]
	\arrow["\otimes"{description}, draw=none, from=2, to=4]
  \end{tikzcd}
  }
  }
  }
}

}
