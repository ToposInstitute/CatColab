{
    "name": "frontend-gaios",
    "version": "0.0.1",
    "description": "version of catcolab that can run in GAIOS",
    "type": "module",
    "main": "src/index.ts",
    "exports": {
        ".": "./dist/index.js"
    },
    "pnpm": {
        "overrides": {
            "solid-js": "^1.9.10",
            "@automerge/automerge-repo": "^2.2.0",
            "@automerge/automerge": "^3.1.1"
        }
    },
    "scripts": {
        "build": "vite build",
        "push": "pnpm build && pnpx pushwork sync",
        "format": "biome format --write",
        "lint": "biome lint --write && biome check --write",
        "watch": "nodemon --watch src -e js,tsx,ts,tsx,css,json --exec 'pnpm build && pnpm push'"
    },
    "keywords": [],
    "author": "Ink & Switch",
    "dependencies": {
        "@automerge/automerge": "^3.1.1",
        "@automerge/automerge-repo": "^2.2.0",
        "@inkandswitch/annotations": "^0.1.4",
        "@inkandswitch/annotations-context": "^0.1.4",
        "@inkandswitch/annotations-diff": "^0.1.4",
        "@inkandswitch/patchwork-refs": "^0.1.3",
        "@inkandswitch/subscribables": "^0.1.2",
        "@inkandswitch/subscribables-solid": "^0.1.1",
        "@patchwork/context": "github:inkandswitch/context",
        "catcolab-ui-components": "workspace:*",
        "catlog-wasm": "link:../catlog-wasm/dist/pkg-browser",
        "frontend": "workspace:*",
        "solid-js": "^1.9.10",
        "subscribables-solid#": "link:@inkandswitch/subscribables-solid#",
        "uuid": "^11.0.3",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.8",
        "@inkandswitch/patchwork-bootloader": "^0.0.4",
        "@types/uuid": "^10.0.0",
        "autoprefixer": "^10.4.20",
        "nodemon": "^3.1.9",
        "pushwork": "^1.0.5",
        "typescript": "^5.2.2",
        "vite": "^5.3.4",
        "vite-plugin-css-injected-by-js": "^3.5.2",
        "vite-plugin-solid": "^2.10.2",
        "vite-plugin-top-level-await": "^1.4.2",
        "vite-plugin-wasm": "^3.3.0"
    }
}
