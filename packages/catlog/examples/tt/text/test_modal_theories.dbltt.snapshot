set_theory ThMulticategory
#/ result: set theory to ThMulticategory

type SigMonoid := [
  M : Object,
  op : Multihom[[M, M], M],
  unit : Multihom[[], M]
]
#/ declared: SigMonoid

generate SigMonoid
#/ result: model generated by 1 object and 2 morphisms
#/ M : Object
#/ op : [M, M] -> M : Multihom
#/ unit : [] -> M : Multihom

type SigRig := [
   R : Object,
   Add : SigMonoid & [ .M := R ],
   Mul : SigMonoid & [ .M := R ]
]
#/ declared: SigRig

generate SigRig
#/ result: model generated by 1 object and 4 morphisms
#/ R : Object
#/ Add.op : [R, R] -> R : Multihom
#/ Add.unit : [] -> R : Multihom
