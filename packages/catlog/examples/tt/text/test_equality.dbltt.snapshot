set_theory ThSchema
#/ result: set theory to ThSchema

type CommutativeSquare := [
    NW : Entity,
    NE : Entity,
    SW : Entity,
    SE : Entity,
    t : (Hom Entity)[NW,NE],
    l : (Hom Entity)[NW,SW],
    r : (Hom Entity)[NE,SE],
    b : (Hom Entity)[SW, SE],
    comm : (t * r == l * b)
]
#/ declared: CommutativeSquare

generate CommutativeSquare
#/ result: model generated by 4 objects and 4 morphisms
#/ NW : Entity
#/ NE : Entity
#/ SW : Entity
#/ SE : Entity
#/ t : NW -> NE : Hom Entity
#/ l : NW -> SW : Hom Entity
#/ r : NE -> SE : Hom Entity
#/ b : SW -> SE : Hom Entity
#/ t ⋅ r = l ⋅ b : Hom Entity(NW,SE)

syn [S : CommutativeSquare] S.comm
#/ result: S.comm : S.t · S.r == S.l · S.b

