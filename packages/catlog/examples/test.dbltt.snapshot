type Graph := [
  V : Entity,
  E : Entity,
  src : (Hom Entity)[E, V],
  tgt : (Hom Entity)[E, V]
]
#/ declared: Graph

def reverse[G : Graph] : Graph := [
  V := G.V,
  E := G.E,
  src := G.tgt,
  tgt := G.src
]
#/ declared: reverse

#(should_fail)
def reverse1[G : Graph] : Graph := [
  V := G.E,
  E := G.E,
  src := G.tgt,
  tgt := G.src
]
