#/ result: G.E

syn [G : Graph] (reverse[G]).src
#/ result: reverse[ G ].src : (Hom Entity)[ G.E, G.V ]

type ReflexiveGraph := [
  V : Entity,
  E : Entity,
  src : (Hom Entity)[E, V],
  tgt : (Hom Entity)[E, V],
  refl : (Hom Entity)[V, E]
]
#/ declared: ReflexiveGraph

def edge_id[G : ReflexiveGraph] : (Hom Entity)[G.E, G.E] := @id G.E
#/ declared: edge_id

def edge_src[G : ReflexiveGraph] : (Hom Entity)[G.E, G.E] := G.src * G.refl
#/ declared: edge_src

