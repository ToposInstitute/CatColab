{
    "name": "catcolab-ui-components",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*"
    },
    "scripts": {
        "format": "biome format --write",
        "lint": "biome lint --write && biome check --write",
        "ci": "biome ci && tsc --noEmit",
