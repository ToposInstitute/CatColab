        "typedoc": "^0.26.8",
        "typescript": "^5.2.2",
        "vite": "^5.3.1",
        "vite-plugin-solid": "^2.10.2",
        "vite-plugin-top-level-await": "^1.4.1",
        "vite-plugin-wasm": "^3.3.0",
        "wasm-pack": "^0.13.0"
