{
    "name": "frontend",
    "private": true,
    "license": "MIT",
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "staging": "vite --mode staging",
        "dev": "vite --host",
        "build:wasm": "npm --prefix ../catlog-wasm run build:browser",
        "build:nix": "tsc -b && vite build --sourcemap true",
        "build": "npm run build:wasm && npm run build:nix",
        "compile": "tcm -p 'src/**/*.module.css' . && tsc -b",
        "preview": "vite preview",
        "format": "biome format --write",
        "lint": "biome lint --write && biome check --write",
        "ci": "biome ci",
        "doc": "typedoc --entryPointStrategy expand ./src",
        "test": "vitest --mode development"
    },
    "dependencies": {
        "@atlaskit/pragmatic-drag-and-drop": "^1.3.0",
        "@atlaskit/pragmatic-drag-and-drop-hitbox": "^1.0.3",
        "@automerge/automerge": "^3.1.1",
        "@automerge/automerge-repo": "^2.2.0",
        "@automerge/automerge-repo-network-websocket": "^2.2.0",
        "@automerge/automerge-repo-storage-indexeddb": "^2.2.0",
        "@automerge/prosemirror": "v0.2.0-alpha.0",
        "@benrbray/prosemirror-math": "^1.0.0",
        "@corvu/dialog": "^0.2.4",
        "@corvu/disclosure": "^0.2.1",
        "@corvu/popover": "^0.2.0",
        "@corvu/resizable": "^0.2.4",
        "@jupyterlab/services": "^7.3.1",
        "@kobalte/core": "^0.13.10",
        "@mdx-js/rollup": "^3.1.0",
        "@modular-forms/solid": "^0.24.1",
        "@qubit-rs/client": "^0.4.5",
        "@solid-primitives/active-element": "^2.1.1",
        "@solid-primitives/context": "^0.3.1",
        "@solid-primitives/destructure": "^0.2.1",
        "@solid-primitives/event-listener": "^2.4.3",
        "@solid-primitives/timer": "^1.4.1",
        "@solidjs/router": "^0.15.3",
        "@viz-js/viz": "^3.15.0",
        "catcolab-api": "link:../backend/pkg",
        "catlog-wasm": "link:../catlog-wasm/dist/pkg-browser",
        "echarts": "^5.5.1",
        "echarts-solid": "^0.2.0",
        "fast-equals": "^5.2.2",
        "fast-json-patch": "^3.1.1",
        "firebase": "^10.14.0",
        "js-file-download": "^0.4.12",
        "katex": "^0.16.22",
        "lucide-solid": "^0.471.0",
        "prosemirror-commands": "^1.7.1",
        "prosemirror-inputrules": "^1.5.0",
        "prosemirror-keymap": "^1.2.3",
        "prosemirror-model": "^1.25.2",
        "prosemirror-schema-list": "^1.5.1",
        "prosemirror-state": "^1.4.3",
        "prosemirror-transform": "^1.10.4",
        "prosemirror-view": "^1.40.1",
        "solid-firebase": "^0.3.0",
        "solid-js": "^1.9.7",
        "tiny-invariant": "^1.3.3",
        "ts-pattern": "^5.2.0",
        "uuid": "^11.0.3"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.3",
        "@mdx-js/mdx": "^2.3.0",
        "@types/mdx": "^2.0.13",
        "rehype-katex": "^7.0.1",
