  "query": "\n        SELECT subject as \"user_id\", username, display_name,\n               level as \"level: PermissionLevel\"\n        FROM permissions\n        LEFT OUTER JOIN users ON id = subject\n        WHERE object = $1\n        AND EXISTS (SELECT 1 FROM refs WHERE id = $1 AND deleted_at IS NULL)\n        ",
