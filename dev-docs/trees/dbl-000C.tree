            \arrow[""{name=6, anchor=center, inner sep=0}, "{Hz}"', "\shortmid"{marking}, from=3-1, to=3-4]
            \arrow[""{name=7, anchor=center, inner sep=0}, "{Hz}"', "\shortmid"{marking}, from=3-6, to=3-9]
